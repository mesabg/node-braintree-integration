{
  "name": "node-braintree-integration",
  "version": "1.0.0",
  "description": "Braintree Integration With NodeJS",
  "main": "src/server.js",
  "scripts": {
    "postinstall": "npm install renderjson bootstrap@4.0.0-alpha.6 jquery tether --prefix=./src/public --verbose",
    "start": "nodemon --watch src -e * --exec npm run serve",
    "lint": "eslint src/",
    "build": "sh compile.sh",
    "preserve": "npm run build",
    "serve": "node dist/server.js"
  },
  "keywords": [
    "Braintree",
    "NodeJS",
    "Payment"
  ],
  "author": "Mois√©s Berenguer",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/langroo/langroo-api.git"
  },
  "engines": {
    "node": "9.2.1",
    "npm": "5.5.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-inline-json": "^1.2.2",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "braintree": "^2.4.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.15.0",
    "eslint-config-zavatta": "^6.0.1",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "express-layout": "^0.1.0",
    "hbs": "^4.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.14.11"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      [
        "inline-json",
        {
          "matchPattern": "config"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "zavatta",
    "rules": {
      "no-console": 0,
      "callback-return": 0
    }
  }
}
